<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="borderpane" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.First_tab_controller">
   <center>
      <TabPane prefHeight="455.0" prefWidth="597.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab text="Highlight procesor">
            <content>
              <AnchorPane fx:id="base_tab_1" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="choose_ebook" layoutX="215.0" layoutY="34.0" mnemonicParsing="false" onAction="#chooseFileEbook" text="Choose ebook File" />
                        <TextField fx:id="ebookSelected" layoutX="34.0" layoutY="34.0" />
                        <Button layoutX="34.0" layoutY="74.0" mnemonicParsing="false" text="Button" />
                        <TextField fx:id="highlightsSelected" layoutX="34.0" layoutY="74.0" />
                        <Button fx:id="Choose_highlight_HTML" layoutX="215.0" layoutY="74.0" mnemonicParsing="false" onAction="#chooseFileHighlights" text="Choose highlights HTML" />
                        <TextArea fx:id="messagesWindow" layoutX="34.0" layoutY="151.0" prefHeight="200.0" prefWidth="278.0" wrapText="true" />
                        <Button layoutX="415.0" layoutY="325.0" mnemonicParsing="false" onAction="#matchHighlights" text="Match highlights" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Untitled Tab 2">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </center>
</BorderPane>
